<view class = 'openBox'>
  <view class = 'openBox-box'>
    <view class='openBox-input-label'>请输入零食盒子编号
    </view>
    <input class='openBox-input' type='number' bindinput='inputBoxNum'></input>
    <navigator url="../box/box" class='openBox-submit' bindtap='openBox' data-boxnum = "{{boxNum}}">确定
    </navigator>
  </view>
</view>